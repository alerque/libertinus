feature locl {

 script cyrl;
     language SRB  exclude_dflt;
      lookup locl_cyrillic;

 script latn;
     language AZE  exclude_dflt;
# #ifndef SANS
      lookup locl_undo_fi;
# #endif
     language CRT  exclude_dflt;
# #ifndef SANS
      lookup locl_undo_fi;
# #endif
     language FIN  exclude_dflt;
      lookup locl_sami;
     language ISM  exclude_dflt;
      lookup locl_sami;
     language LSM  exclude_dflt;
      lookup locl_sami;
     language NOR  exclude_dflt;
      lookup locl_sami;
     language NSM  exclude_dflt;
      lookup locl_sami;
     language SKS  exclude_dflt;
      lookup locl_sami;
     language SSM  exclude_dflt;
      lookup locl_sami;
     language SVE  exclude_dflt;
      lookup locl_sami;
     language TRK  exclude_dflt;
      lookup locl_sami;
# #ifndef SANS
      lookup locl_undo_fi;
# #endif
} locl;

