     language dflt ;
      lookup smcp_smallcap_i;
      lookup smcp_smallcap;

 script grek;
     language dflt ;
      lookup smcp_smallcap_i;
      lookup smcp_smallcap;

 script latn;
     language dflt ;
      lookup smcp_smallcap_i;
      lookup smcp_smallcap;
     language AZE  exclude_dflt;
      lookup smcp_smallcap_i;
      lookup smcp_smallcap;
      lookup smcp_smallcap_idot;
     language CRT  exclude_dflt;
      lookup smcp_smallcap_i;
      lookup smcp_smallcap;
      lookup smcp_smallcap_idot;
     language TRK  exclude_dflt;
      lookup smcp_smallcap_i;
      lookup smcp_smallcap;
      lookup smcp_smallcap_idot;
} smcp;

